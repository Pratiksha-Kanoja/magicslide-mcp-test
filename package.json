{
  "name": "magicslide-mcp-test",
  "version": "1.0.0",
  "description": "A Model Context Protocol server example",
  "private": true,
  "module": "./src/index.ts",
  "engines": {
    "node": ">=18 <23"
  },
  "bin": {
    "mcp-server": "build/index.js"
  },
  "files": [
    "build",
    "src",
    "tsconfig.json",
    "smithery.yaml",
    "package.json",
    "README.md"
  ],
